endif

ifeq ($(CONFIG),Release)
  BINDIR := ../../lib/gcc_dll
  LIBDIR := ../../lib/gcc_lib
  OBJDIR := .objsu
  OUTDIR := ../../lib/gcc_lib
  CPPFLAGS := -MMD -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "__WXMSW__" -D "WIN32" -D "_WINDOWS" -D "NDEBUG" -I "../../include" -I "$(WXWIN)/include" -I "$(WXWIN)/lib/gcc_dll/mswu"
