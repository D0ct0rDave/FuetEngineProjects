ifndef CONFIG
  CONFIG=Debug
endif

ifeq ($(CONFIG),Debug)
  BINDIR := ../../lib/gcc_dll
  LIBDIR := ../../lib/gcc_lib
  OBJDIR := .objsud
  OUTDIR := ../../lib/gcc_lib
  CPPFLAGS := -MMD -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "__WXMSW__" -D "WIN32" -D "_WINDOWS" -D "DEBUG" -D "_DEBUG" -I "../../include" -I "$(WXWIN)/include" -I "$(WXWIN)/lib/gcc_dll/mswud"
